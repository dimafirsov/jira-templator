{
  "name": "jit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng b --prod && npm run copy-files",
    "build-normal": "ng b && npm run build-scripts && npm run copy-files",
    "build-scripts": "npm run build-content-script && npm run build-background-script",
    "build-content-script": "esbuild scripts/contentScript.js --bundle --outfile=dist/contentScript.js --minify",
    "build-background-script": "esbuild scripts/backgroundScript.js --bundle --outfile=dist/backgroundScript.js --minify",
    "copy-files": "cpx manifest.json dist && cpx package.json dist",
    "pack-clean": "cd ../ && rm -rf ./jit-*.tgz",
    "pack-it": "cd ./dist && npm pack && cpx jit-*.tgz ../../",
    "remove-dist": "rm -rf ./dist",
    "test": "ng test",
    "test-ci": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "merge:ci": "bash scripts/merge",
    "trigger-pipeline:ci": "bash scripts/trigger-build-pipeline"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2 || ^12.0.0",
    "@angular/cdk": "^11.1.1 || ^12.0.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "^11.1.2 || ^12.0.0",
    "@angular/forms": "^11.1.2 || ^12.0.0",
    "@angular/localize": "^11.1.2 || ^12.0.0",
    "@angular/platform-browser": "^11.1.2 || ^12.0.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "^11.1.2 || ^12.0.0",
    "@nova-ui/bits": "^11.11.0",
    "@types/chrome": "0.0.139",
    "bootstrap-icons": "^1.5.0",
    "file-saver": "^2.0.5",
    "highlight.js": "^10.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.27",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cdk": "^11.1.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@types/file-saver": "^2.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.11.1",
    "bootstrap": "^5.0.0-beta3",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.13.14",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
